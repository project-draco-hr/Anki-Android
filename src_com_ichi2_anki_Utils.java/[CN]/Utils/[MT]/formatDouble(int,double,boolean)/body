{
  String start;
  String end;
  if (bold) {
    start="<b>";
    end="</b>";
  }
 else {
    start="";
    end="";
  }
  if (type == 0 || (adjustedInterval * 10) % 10 == 0) {
    return start + String.valueOf((int)adjustedInterval) + end;
  }
 else {
    return start + String.valueOf(adjustedInterval) + end;
  }
}
