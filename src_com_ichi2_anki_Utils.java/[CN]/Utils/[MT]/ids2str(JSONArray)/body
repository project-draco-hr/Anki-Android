{
  String str="(";
  if (ids != null) {
    int len=ids.length();
    for (int i=0; i < len; i++) {
      try {
        if (i == (len - 1)) {
          str+=ids.get(i);
        }
 else {
          str+=ids.get(i) + ",";
        }
      }
 catch (      JSONException e) {
        Log.i(TAG,"JSONException = " + e.getMessage());
      }
    }
  }
  str+=")";
  return str;
}
