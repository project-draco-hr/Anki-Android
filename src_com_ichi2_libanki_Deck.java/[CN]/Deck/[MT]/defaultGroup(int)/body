{
  if (id == 1) {
    return 1;
  }
  Cursor cur=null;
  try {
    cur=getDB().getDatabase().rawQuery("SELECT id FROM groups WHERE id = " + id,null);
    if (cur.moveToNext()) {
      return cur.getInt(0);
    }
 else {
      return 1;
    }
  }
  finally {
    if (cur != null && !cur.isClosed()) {
      cur.close();
    }
  }
}
