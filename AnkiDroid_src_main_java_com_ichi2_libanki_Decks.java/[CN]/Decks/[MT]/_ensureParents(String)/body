{
  String s="";
  String[] path=name.split("::");
  if (path.length < 2) {
    return name;
  }
  for (int i=0; i < path.length - 1; i++) {
    if (i == 0) {
      s=path[0];
    }
 else {
      s=s + "::" + path[i];
    }
    long did=id(s);
    s=name(did);
  }
  name=s + "::" + path[path.length - 1];
  return name;
}
