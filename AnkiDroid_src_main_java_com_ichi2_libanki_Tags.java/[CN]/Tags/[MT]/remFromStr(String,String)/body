{
  List<String> currentTags=split(tags);
  for (  String tag : split(deltags)) {
    List<String> remove=new ArrayList<String>();
    for (    String tx : currentTags) {
      if (tag.equalsIgnoreCase(tx)) {
        remove.add(tx);
      }
    }
    for (    String r : remove) {
      currentTags.remove(r);
    }
  }
  return join(currentTags);
}
