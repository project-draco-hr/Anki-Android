{
  Intent intent=new Intent(action);
  Uri uri=data == null ? null : Uri.parse(data);
  if (type == null) {
    intent.setData(uri);
  }
 else {
    intent.setDataAndType(uri,type);
  }
  for (  Map.Entry<String,String> entry : extras.entrySet()) {
    intent.putExtra(entry.getKey(),entry.getValue());
  }
  return intent;
}
