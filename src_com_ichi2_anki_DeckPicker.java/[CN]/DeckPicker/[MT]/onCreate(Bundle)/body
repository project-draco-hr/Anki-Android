{
  super.onCreate(savedInstanceState);
  mSelf=this;
  setContentView(R.layout.main);
  mFileList=(ListView)findViewById(R.id.files);
  mFileListAdapter=new ArrayAdapter<FileBrowser.FileEntry>(this,R.layout.main_fileentry);
