{
  super.onCollectionLoaded(col);
  Boolean started=BackupManager.performBackupInBackground(col.getPath());
  if (started) {
  }
  if (mFragmented) {
    long did=col.getDecks().selected();
    selectDeck(did);
  }
  if (!mFragmented) {
    mShowShowcaseView=true;
  }
  AnkiDroidApp.getCompat().invalidateOptionsMenu(this);
  loadCounts();
}
