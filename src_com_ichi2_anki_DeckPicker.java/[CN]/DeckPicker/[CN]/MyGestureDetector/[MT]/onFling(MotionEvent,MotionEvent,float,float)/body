{
  if (mSwipeEnabled) {
    try {
      if (e1.getX() - e2.getX() > sSwipeMinDistance && Math.abs(velocityX) > sSwipeThresholdVelocity && Math.abs(e1.getY() - e2.getY()) < sSwipeMaxOffPath) {
        openStudyOptions();
      }
    }
 catch (    Exception e) {
      Log.e(AnkiDroidApp.TAG,"onFling Exception = " + e.getMessage());
    }
  }
  return false;
}
