{
  Collection col=params[0].getCollection();
  if (col != null) {
    try {
      WidgetStatus.waitToFinish();
      String path=col.getPath();
      AnkiDroidApp.closeCollection(true);
      BackupManager.performBackupInBackground(path);
    }
 catch (    RuntimeException e) {
    }
  }
  return null;
}
