{
  if (yyyy < Leap100RuleYYYY) {
    if (yyyy < 0) {
      return ((yyyy + 1) & 3) == 0;
    }
 else {
      return (yyyy & 3) == 0;
    }
  }
  if ((yyyy & 3) != 0) {
    return false;
  }
  if (yyyy % 100 != 0) {
    return true;
  }
  if (yyyy < Leap400RuleYYYY) {
    return false;
  }
  return yyyy % 400 == 0;
}
