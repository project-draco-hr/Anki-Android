{
  sLock.lock();
  try {
    if (!mIsRunning) {
      return null;
    }
 else     mImageView=(ChartView)params[0];
    ChartBuilder chartBuilder=new ChartBuilder(mImageView,AnkiDroidApp.getCol(),sIsWholeCollection,Stats.ChartType.OTHER);
    return chartBuilder.createSmallDueChart(mSeriesList);
  }
  finally {
    sLock.unlock();
  }
}
