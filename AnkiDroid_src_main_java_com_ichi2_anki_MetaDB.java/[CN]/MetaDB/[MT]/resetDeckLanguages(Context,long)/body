{
  openDBIfClosed(context);
  try {
    mMetaDb.execSQL("DELETE FROM languages WHERE did = " + did + ";");
    Log.i(AnkiDroidApp.TAG,"Resetting language assignment for deck " + did);
    return true;
  }
 catch (  Exception e) {
    Log.e("Error","Error resetting deck language",e);
  }
  return false;
}
