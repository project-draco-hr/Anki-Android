{
  if (mMetaDb == null || !mMetaDb.isOpen()) {
    openDB(context);
  }
  try {
    Timber.i("Resetting intent information");
    mMetaDb.execSQL("DROP TABLE IF EXISTS intentInformation;");
    upgradeDB(mMetaDb,DATABASE_VERSION);
    return true;
  }
 catch (  Exception e) {
    Timber.e(e,"Error resetting intentInformation ");
  }
  return false;
}
