{
  try {
    if (mCol.getConf().getInt("newSpread") == NEW_CARDS_DISTRIBUTE) {
      if (mNewCount != 0) {
        mNewCardModulus=(mNewCount + mRevCount) / mNewCount;
        if (mRevCount != 0) {
          mNewCardModulus=Math.max(2,mNewCardModulus);
        }
        return;
      }
    }
    mNewCardModulus=0;
  }
 catch (  JSONException e) {
    throw new RuntimeException(e);
  }
}
