{
  if (card.getODid() == 0 && card.getODue() != 0) {
    JSONObject conf=_lapseConf(card);
    try {
      if (conf.getJSONArray("delays").length() > 1) {
        return 3;
      }
    }
 catch (    JSONException e) {
      throw new RuntimeException(e);
    }
    return 2;
  }
  if (card.getQueue() == 2) {
    return 4;
  }
 else {
    return 3;
  }
}
