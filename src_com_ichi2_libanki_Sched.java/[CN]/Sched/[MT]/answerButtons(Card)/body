{
  if (card.getODue() != 0) {
    if (card.getODid() != 0 && card.getQueue() == 2) {
      return 4;
    }
    JSONObject conf=_lapseConf(card);
    try {
      if (card.getType() == 0 || conf.getJSONArray("delays").length() > 1) {
        return 3;
      }
    }
 catch (    JSONException e) {
      throw new RuntimeException(e);
    }
    return 2;
  }
 else   if (card.getQueue() == 2) {
    return 4;
  }
 else {
    return 3;
  }
}
