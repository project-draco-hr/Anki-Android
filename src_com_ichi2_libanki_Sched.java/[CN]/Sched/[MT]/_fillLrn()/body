{
  if (mLrnCount == 0) {
    return false;
  }
  if (!mLrnQueue.isEmpty()) {
    return true;
  }
  Cursor cur=null;
  mLrnQueue.clear();
  try {
    cur=mCol.getDb().getDatabase().rawQuery("SELECT due, id FROM cards WHERE did IN " + _deckLimit() + " AND queue = 1 AND due < "+ mDayCutoff+ " SORT BY due LIMIT "+ mReportLimit,null);
    while (cur.moveToNext()) {
      mLrnQueue.add(new long[]{cur.getInt(0),cur.getLong(1)});
    }
    return !mLrnQueue.isEmpty();
  }
  finally {
    if (cur != null && !cur.isClosed()) {
      cur.close();
    }
  }
}
