{
  if (now == 0) {
    now=Utils.intNow();
  }
  int ok=0;
  for (int i=0; i < left; i++) {
    try {
      now+=(int)(delays.getDouble(delays.length() - left + i) * 60.0);
    }
 catch (    JSONException e) {
      throw new RuntimeException(e);
    }
    if (now > mDayCutoff) {
      break;
    }
    ok=i;
  }
  return ok + 1;
}
