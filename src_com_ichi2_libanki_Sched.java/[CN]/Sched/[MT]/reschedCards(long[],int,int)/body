{
  ArrayList<Object[]> d=new ArrayList<Object[]>();
  int t=mToday;
  long mod=Utils.intNow();
  Random rnd=new Random();
  for (  long id : ids) {
    int r=rnd.nextInt(imax - imin + 1) + imin;
    d.add(new Object[]{Math.max(1,r),r + t,mCol.usn(),mod,2500,id});
  }
  mCol.getDb().executeMany("update cards set type=2,queue=2,ivl=?,due=?, " + "usn=?, mod=?, factor=? where id=? and odid=0",d);
}
