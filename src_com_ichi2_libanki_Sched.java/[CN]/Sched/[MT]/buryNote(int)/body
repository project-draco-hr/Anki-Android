{
  mCol.setDirty();
  ArrayList<Integer> cids=mCol.getDb().queryColumn(Integer.class,"SELECT id FROM card WHERE nid = " + nid,0);
  int[] ids=new int[cids.size()];
  int i=0;
  for (  int c : cids) {
    ids[i]=c;
    i++;
  }
  removeFailed(ids);
  mCol.getDb().getDatabase().execSQL("UPDATE cards SET queue = -2 WHERE nid = " + nid);
}
