{
  mCol.setDirty();
  ArrayList<Long> cids=mCol.getDb().queryColumn(Long.class,"SELECT id FROM cards WHERE nid = " + nid,0);
  long[] ids=new long[cids.size()];
  int i=0;
  for (  long c : cids) {
    ids[i]=c;
    i++;
  }
  removeFailed(ids);
  mCol.getDb().execute("UPDATE cards SET queue = -2 WHERE nid = " + nid);
}
