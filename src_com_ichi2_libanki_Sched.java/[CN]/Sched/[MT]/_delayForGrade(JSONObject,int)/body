{
  left=left % 1000;
  try {
    double delay;
    JSONArray ja=conf.getJSONArray("delays");
    int len=ja.length();
    try {
      delay=ja.getDouble(len - left);
    }
 catch (    JSONException e) {
      if (conf.has("delays")) {
        delay=conf.getJSONArray("delays").getDouble(0);
      }
 else {
        delay=1;
      }
      delay=ja.getDouble(0);
    }
    return (int)(delay * 60.0);
  }
 catch (  JSONException e) {
    throw new RuntimeException(e);
  }
}
