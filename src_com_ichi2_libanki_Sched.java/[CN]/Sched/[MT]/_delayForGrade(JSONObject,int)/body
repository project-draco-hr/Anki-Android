{
  try {
    int delay;
    JSONArray ja=conf.getJSONArray("delays");
    int len=ja.length();
    if (len < left) {
      delay=ja.getInt(0);
    }
 else {
      delay=conf.getJSONArray("delays").getInt(len - left);
    }
    return delay * 60;
  }
 catch (  JSONException e) {
    throw new RuntimeException(e);
  }
}
