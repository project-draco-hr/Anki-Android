{
  double lastSync=0;
  if (connect(false) != LOGIN_OK) {
    return -1.0;
  }
  try {
    JSONArray deckInfo=mDecks.getJSONArray(mDeckName);
    lastSync=deckInfo.getDouble(1);
  }
 catch (  JSONException e) {
    Log.e(AnkiDroidApp.TAG,"lastSync - JSONException = " + e.getMessage());
    return -1.0;
  }
  return lastSync;
}
