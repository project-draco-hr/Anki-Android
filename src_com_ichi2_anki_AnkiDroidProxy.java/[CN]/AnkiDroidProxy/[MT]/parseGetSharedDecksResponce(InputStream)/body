{
  BufferedReader rd=new BufferedReader(new InputStreamReader(is),409600);
  JsonReader reader=new JsonReader(rd);
  List<SharedDeck> sharedDecks=new ArrayList<SharedDeck>();
  try {
    reader.beginArray();
    int count=0;
    while (reader.hasNext()) {
      SharedDeck sd=new SharedDeck();
      reader.beginArray();
      sd.setId(reader.nextInt());
      reader.skipValue();
      sd.setTitle(reader.nextString());
      reader.skipValue();
      reader.skipValue();
      reader.skipValue();
      sd.setFacts(reader.nextInt());
      sd.setSize(reader.nextInt());
      reader.skipValue();
      reader.skipValue();
      reader.skipValue();
      reader.endArray();
      sharedDecks.add(sd);
      count++;
    }
    reader.endArray();
    reader.close();
    Log.d(AnkiDroidApp.TAG,"parseGetSharedDecksResponce: found " + count + " shared decks");
  }
 catch (  Exception e) {
    Log.e(AnkiDroidApp.TAG,Log.getStackTraceString(e));
    sharedDecks.clear();
    throw e;
  }
  return sharedDecks;
}
