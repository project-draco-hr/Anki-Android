{
  StringBuilder str=new StringBuilder(512);
  str.append("(");
  if (ids != null) {
    int len=ids.size();
    for (int i=0; i < len; i++) {
      if (i == (len - 1)) {
        str.append(ids.get(i));
      }
 else {
        str.append(ids.get(i)).append(",");
      }
    }
  }
  str.append(")");
  return str.toString();
}
