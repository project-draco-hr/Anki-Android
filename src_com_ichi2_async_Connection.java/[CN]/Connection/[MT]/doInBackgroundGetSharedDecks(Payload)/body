{
  try {
    data.result=AnkiDroidProxy.getSharedDecks();
  }
 catch (  Exception e) {
    data.success=false;
    data.exception=e;
    Log.e(AnkiDroidApp.TAG,"doInBackgroundGetSharedDecks - Error getting shared decks = " + e.getMessage());
    Log.e(AnkiDroidApp.TAG,Log.getStackTraceString(e));
  }
  return data;
}
