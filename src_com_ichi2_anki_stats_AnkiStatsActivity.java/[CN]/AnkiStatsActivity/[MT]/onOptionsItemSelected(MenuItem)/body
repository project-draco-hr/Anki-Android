{
  if (getDrawerToggle().onOptionsItemSelected(item)) {
    return true;
  }
  int itemId=item.getItemId();
switch (itemId) {
case R.id.action_time_spinner:
    showTimeDialog();
  return true;
case R.id.action_deck_spinner:
showDeckDialog();
return true;
}
return super.onOptionsItemSelected(item);
}
