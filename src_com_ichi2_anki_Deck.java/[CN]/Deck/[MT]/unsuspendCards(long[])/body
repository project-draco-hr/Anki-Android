{
  AnkiDatabaseManager.getDatabase(mDeckPath).getDatabase().execSQL("UPDATE cards SET " + "priority = 0, " + "modified = " + String.format(ENGLISH_LOCALE,"%f",(double)(System.currentTimeMillis() / 1000.0)) + " WHERE id IN "+ Utils.ids2str(ids));
  updatePriorities(ids);
  rebuildCounts(false);
  flushMod();
}
