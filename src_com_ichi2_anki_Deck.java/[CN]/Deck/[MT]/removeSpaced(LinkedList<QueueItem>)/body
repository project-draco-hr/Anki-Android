{
  while (!queue.isEmpty()) {
    long fid=((QueueItem)queue.getLast()).getFactID();
    if (spacedFacts.containsKey(fid)) {
      if ((double)System.currentTimeMillis() / 1000.0 > spacedFacts.get(fid)) {
        spacedFacts.remove(fid);
      }
 else {
        queue.removeLast();
      }
    }
 else {
      return;
    }
  }
}
