{
  while (!queue.isEmpty()) {
    long fid=((QueueItem)queue.getLast()).getFactID();
    if (mSpacedFacts.containsKey(fid)) {
      if (Utils.now() > mSpacedFacts.get(fid)) {
        mSpacedFacts.remove(fid);
      }
 else {
        queue.removeLast();
      }
    }
 else {
      return;
    }
  }
}
