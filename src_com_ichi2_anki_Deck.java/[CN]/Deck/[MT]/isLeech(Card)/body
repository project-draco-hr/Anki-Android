{
  int no=card.getNoCount();
  int fmax=0;
  if (hasKey("leechFails")) {
    fmax=getInt("leechFails");
    if (fmax == 0) {
      return false;
    }
  }
 else {
    return false;
  }
  if (!card.isRev() && (no >= fmax) && (((double)(fmax - no)) % Math.max(fmax / 2,1) == 0)) {
    return true;
  }
 else {
    return false;
  }
}
