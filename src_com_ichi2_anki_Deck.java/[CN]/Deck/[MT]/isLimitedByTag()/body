{
  if (!getVar("newActive").equals("")) {
    return true;
  }
 else   if (!getVar("newInactive").equals("")) {
    return true;
  }
 else   if (!getVar("revActive").equals("")) {
    return true;
  }
 else   if (!getVar("revInactive").equals("")) {
    return true;
  }
 else {
    return false;
  }
}
