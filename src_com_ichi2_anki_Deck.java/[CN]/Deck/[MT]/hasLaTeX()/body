{
  Cursor cursor=null;
  try {
    cursor=getDB().getDatabase().rawQuery("SELECT Id FROM fields WHERE " + "(value like '%[latex]%[/latex]%') OR " + "(value like '%[$]%[/$]%') OR "+ "(value like '%[$$]%[/$$]%') LIMIT 1 ",null);
    if (cursor.moveToFirst()) {
      return true;
    }
  }
  finally {
    if (cursor != null) {
      cursor.close();
    }
  }
  return false;
}
