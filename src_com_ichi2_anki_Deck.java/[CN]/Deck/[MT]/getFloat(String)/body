{
  if (mDeckVars.containsKey(key)) {
    try {
      return Double.parseDouble(mDeckVars.get(key));
    }
 catch (    NumberFormatException e) {
      Log.w(AnkiDroidApp.TAG,"NumberFormatException: Converting deckvar to double failed, key: \"" + key + "\", value: \""+ mDeckVars.get(key)+ "\"");
      return 0.0;
    }
  }
 else {
    return 0.0;
  }
}
