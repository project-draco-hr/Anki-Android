{
  Log.i(TAG,"deleteDanglingFacts");
  ArrayList<String> danglingFacts=getDB().queryColumn(String.class,"SELECT facts.id FROM facts WHERE facts.id NOT IN (SELECT DISTINCT factId from cards)",0);
  if ((danglingFacts != null) && (danglingFacts.size() > 0)) {
    deleteFacts(danglingFacts);
  }
  return danglingFacts;
}
