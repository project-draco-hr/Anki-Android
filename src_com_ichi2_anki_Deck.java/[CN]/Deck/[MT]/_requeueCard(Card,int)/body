{
  try {
    if (card.reps == 1) {
      newQueue.removeLast();
    }
 else     if (oldSuc == 0) {
      failedQueue.removeLast();
    }
 else {
      revQueue.removeLast();
    }
  }
 catch (  Exception e) {
    throw new RuntimeException("requeueCard() failed. Counts: " + failedSoonCount + " "+ revCount+ " "+ newCountToday+ ", Queue: "+ failedQueue.size()+ " "+ revQueue.size()+ " "+ newQueue.size()+ ", Card info: "+ card.reps+ " "+ card.successive+ " "+ oldSuc);
  }
}
