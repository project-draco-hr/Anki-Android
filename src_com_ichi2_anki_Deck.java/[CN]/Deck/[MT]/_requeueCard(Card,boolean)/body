{
  int newType=0;
  if (card.getReps() == 1) {
    if (mNewFromCache) {
      newType=2;
      ArrayList<Long> cards=mSpacedCards.remove().getCards();
      if (cards.size() > 1) {
        cards.remove(0);
        mSpacedCards.addLast(new SpacedCardsItem(Utils.now() + mNewSpacing,cards));
      }
    }
 else {
      newType=1;
      mNewQueue.removeLast();
    }
  }
 else   if (!oldIsRev) {
    mFailedQueue.removeLast();
  }
 else {
    mRevQueue.removeLast();
  }
}
