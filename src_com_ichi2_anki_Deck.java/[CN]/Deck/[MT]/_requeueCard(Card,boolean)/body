{
  try {
    if (card.getReps() == 1) {
      newQueue.removeLast();
    }
 else     if (!oldIsRev) {
      failedQueue.removeLast();
    }
 else {
      revQueue.removeLast();
    }
  }
 catch (  Exception e) {
    throw new RuntimeException("requeueCard() failed. Counts: " + mFailedSoonCount + " "+ mRevCount+ " "+ mNewCountToday+ ", Queue: "+ failedQueue.size()+ " "+ revQueue.size()+ " "+ newQueue.size()+ ", Card info: "+ card.getReps()+ " "+ card.isRev()+ " "+ oldIsRev);
  }
}
