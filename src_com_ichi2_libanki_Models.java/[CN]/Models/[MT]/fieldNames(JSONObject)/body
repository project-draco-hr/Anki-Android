{
  JSONArray ja;
  try {
    ja=m.getJSONArray("flds");
    ArrayList<String> names=new ArrayList<String>();
    for (int i=0; i < ja.length(); i++) {
      names.add(ja.getJSONObject(i).getString("name"));
    }
    return names;
  }
 catch (  JSONException e) {
    throw new RuntimeException(e);
  }
}
