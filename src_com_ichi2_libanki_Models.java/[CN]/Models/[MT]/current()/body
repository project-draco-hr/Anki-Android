{
  JSONObject m;
  try {
    m=get(mCol.getDecks().current().getLong("mid"));
    if (m == null) {
      m=get(mCol.getConf().getLong("curModel"));
    }
    if (m == null) {
      if (!mModels.isEmpty()) {
        m=mModels.values().iterator().next();
      }
    }
    return m;
  }
 catch (  JSONException e) {
    throw new RuntimeException(e);
  }
}
