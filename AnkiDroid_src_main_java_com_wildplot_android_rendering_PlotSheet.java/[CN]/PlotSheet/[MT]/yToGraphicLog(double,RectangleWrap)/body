{
  return (float)(((Math.log10(y) - Math.log10(this.screenParts.get(currentScreen).getyRange()[0])) / (Math.log10(this.screenParts.get(currentScreen).getyRange()[1]) - Math.log10(this.screenParts.get(currentScreen).getyRange()[0]))) * (field.height - upperFrameThickness - bottomFrameThickness) - (field.height - upperFrameThickness - bottomFrameThickness)) * (-1) + upperFrameThickness;
}
