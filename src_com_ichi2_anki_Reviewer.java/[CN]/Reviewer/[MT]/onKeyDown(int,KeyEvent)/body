{
  if (keyCode == KeyEvent.KEYCODE_BACK && event.getRepeatCount() == 0) {
    Log.i(AnkiDroidApp.TAG,"Reviewer - onBackPressed()");
    closeReviewer();
    return true;
  }
  if (keyCode == 92) {
    mCard.pageUp(false);
    return true;
  }
  if (keyCode == 93) {
    mCard.pageDown(false);
    return true;
  }
  return super.onKeyDown(keyCode,event);
}
