{
  super.onConfigurationChanged(newConfig);
  Log.i(AnkiDroidApp.TAG,"onConfigurationChanged");
  mCardTemplate=mCardTemplate.replaceFirst("var availableWidth = \\d*;","var availableWidth = " + getAvailableWidthInCard() + ";");
  refreshCard();
  if (mPrefWhiteboard) {
    mWhiteboard.rotate();
  }
}
