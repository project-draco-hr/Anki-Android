{
  return Math.pow(10,((y - upperFrameThickness + (field.height - upperFrameThickness - bottomFrameThickness)) * (-1)) / ((field.height - upperFrameThickness - bottomFrameThickness)) * ((Math.log10(this.screenParts.get(currentScreen).getyRange()[1]) - Math.log10(this.screenParts.get(currentScreen).getyRange()[0]))) + Math.log10(this.screenParts.get(currentScreen).getyRange()[0]));
}
