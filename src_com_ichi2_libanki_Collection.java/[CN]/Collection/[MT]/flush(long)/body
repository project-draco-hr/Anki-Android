{
  mMod=(mod == 0 ? Utils.intNow(1000) : mod);
  ContentValues values=new ContentValues();
  values.put("crt",mCrt);
  values.put("mod",mMod);
  values.put("scm",mScm);
  values.put("dty",mDty ? 1 : 0);
  values.put("usn",mUsn);
  values.put("ls",mLs);
  values.put("conf",Utils.jsonToString(mConf));
  mDb.update("col",values);
}
