{
  String[] tables=new String[]{"notes","cards","revlog","graves"};
  for (  String t : tables) {
    mDb.getDatabase().execSQL("UPDATE " + t + " SET usn=0 WHERE usn=-1");
  }
  mUsn+=1;
  mModels.beforeUpload();
  mTags.beforeUpload();
  mDecks.beforeUpload();
  modSchema();
  mLs=mScm;
  close();
}
