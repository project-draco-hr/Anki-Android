{
  mDb=db;
  mServer=server;
  mLastSave=Utils.now();
  clearUndo();
  mPath=path;
  mMedia=new Media(this,server);
  mModels=new Models(this);
  mDecks=new Decks(this);
  mTags=new Tags(this);
  load();
  if (mCrt == 0) {
    mCrt=UIUtils.getDayStart() / 1000;
  }
  mStartReps=0;
  mStartTime=0;
  mOvertime=false;
  mSched=new Sched(this);
  cleanup();
}
