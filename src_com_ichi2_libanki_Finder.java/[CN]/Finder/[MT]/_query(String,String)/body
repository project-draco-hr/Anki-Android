{
  String sql;
  if (!preds.contains("n.") && !order.contains("n.")) {
    sql="select c.id from cards c where ";
  }
 else {
    sql="select c.id from cards c, notes n where c.nid=n.id and ";
  }
  if (preds.length() != 0) {
    sql+="(" + preds + ")";
  }
 else {
    sql+="1";
  }
  if (order != null && order.length() != 0) {
    sql+=" " + order;
  }
  return sql;
}
