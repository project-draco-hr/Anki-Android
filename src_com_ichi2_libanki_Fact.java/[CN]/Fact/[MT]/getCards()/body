{
  ArrayList<Card> cards=new ArrayList<Card>();
  Cursor cur=null;
  try {
    cur=mDeck.getDB().getDatabase().rawQuery("SELECT id FROM cards WHERE fid = " + mId,null);
    while (cur.moveToNext()) {
      cards.add(mDeck.getCard(cur.getInt(0)));
    }
  }
  finally {
    if (cur != null) {
      cur.close();
    }
  }
  return (Card[])cards.toArray();
}
