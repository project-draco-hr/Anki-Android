{
  ArrayList<Card> cards=card.note().cards();
  int pos;
  for (  Card c : cards) {
    pos=getPosition(mCards,c.getId());
    if (pos >= 0 && pos < mCards.size()) {
      mCards.remove(pos);
    }
  }
  pos=getPosition(mCards,card.getId());
  if (pos >= 0 && pos < mCards.size()) {
    mCards.remove(pos);
  }
  updateList();
}
