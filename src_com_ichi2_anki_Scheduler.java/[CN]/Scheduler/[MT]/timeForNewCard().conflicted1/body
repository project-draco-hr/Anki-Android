{
  try {
    return ((Boolean)timeForNewCardMethod.invoke(Deck.this)).booleanValue();
  }
 catch (  IllegalArgumentException e) {
    throw new RuntimeException(e);
  }
catch (  IllegalAccessException e) {
    throw new RuntimeException(e);
  }
