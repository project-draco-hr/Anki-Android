{
  Deck deck=params[0].getDeck();
  int chunk=params[0].getInt();
  String startId="";
  while (true) {
    ArrayList<HashMap<String,String>> cards=deck.getCards(chunk,startId);
    if (cards.size() == 0) {
      return null;
    }
 else {
      publishProgress(new TaskData(cards));
      startId=cards.get(cards.size() - 1).get("id");
    }
  }
}
