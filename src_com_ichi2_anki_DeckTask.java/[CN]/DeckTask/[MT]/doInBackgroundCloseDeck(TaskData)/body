{
  Log.i(AnkiDroidApp.TAG,"doInBackgroundCloseDeck");
  Deck deck=params[0].getDeck();
  boolean wait=params[0].getBoolean();
  if (deck != null) {
    try {
      deck.closeDeck(wait);
    }
 catch (    SQLiteException e) {
      Log.e(AnkiDroidApp.TAG,"Error on closing deck: " + e);
    }
  }
  return null;
}
