{
  int taken;
  try {
    taken=Math.min(card.timeTaken(),_cardConf(card).getInt("maxTaken") * 1000);
  }
 catch (  JSONException e) {
    throw new RuntimeException(e);
  }
  mDb.getDatabase().execSQL("INSERT INTO revlog VALUES (" + (int)(Utils.now() * 1000) + ", "+ card.getId()+ ", "+ ease+ ", "+ card.getIvl()+ ", "+ card.getLastIvl()+ ", "+ card.getFactor()+ ", "+ taken+ ", "+ 1+ ")");
}
