{
  JSONArray conf;
  try {
    conf=mDeck.getQconf().getJSONArray("groups");
    if (conf.length() != 0) {
      mDeck.getQconf().put("groups",new JSONArray());
      _resetCounts();
      mDeck.getQconf().put("groups",conf);
    }
  }
 catch (  JSONException e) {
    throw new RuntimeException(e);
  }
  return counts();
}
