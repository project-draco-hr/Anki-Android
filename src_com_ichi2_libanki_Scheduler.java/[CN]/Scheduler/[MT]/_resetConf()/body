{
  Cursor cur=null;
  try {
    cur=mDb.getDatabase().rawQuery("SELECT id, gcid FROM groups",null);
    while (cur.moveToNext()) {
      mGroupConfs.put(cur.getInt(0),cur.getInt(1));
    }
  }
  finally {
    if (cur != null && !cur.isClosed()) {
      cur.close();
    }
  }
  mConfCache.clear();
}
