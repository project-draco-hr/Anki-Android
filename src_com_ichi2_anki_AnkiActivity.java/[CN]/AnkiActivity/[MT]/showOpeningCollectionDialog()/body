{
  if (mOpenCollectionDialog == null || !mOpenCollectionDialog.isShowing()) {
    mOpenCollectionDialog=StyledOpenCollectionDialog.show(AnkiActivity.this,getResources().getString(R.string.open_collection),new OnCancelListener(){
      @Override public void onCancel(      DialogInterface arg0){
        finishWithoutAnimation();
      }
    }
);
  }
}
