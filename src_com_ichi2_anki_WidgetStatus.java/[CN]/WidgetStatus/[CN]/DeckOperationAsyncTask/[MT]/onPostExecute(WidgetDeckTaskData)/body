{
  Log.d(AnkiDroidApp.TAG,"WidgetStatus.DeckOperationAsyncTask.onPostExecute()");
switch (sDeckOperationType) {
case TASK_OPEN_DECK:
    AnkiDroidWidgetBig.setDeckAndLoadCard(params.getDeck());
  break;
case TASK_CLOSE_DECK:
break;
case TASK_ANSWER_CARD:
break;
case TASK_UNDO:
break;
case TASK_BURY_CARD:
break;
}
}
