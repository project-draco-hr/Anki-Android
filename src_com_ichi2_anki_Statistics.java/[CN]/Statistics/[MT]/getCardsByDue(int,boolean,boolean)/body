{
  Deck deck=AnkiDroidApp.deck();
  double series[]=new double[length];
  for (int i=0; i < length; i++) {
    int count=0;
    if (!matureCards) {
      count=deck.getNextDueCards(i);
    }
 else {
      count=deck.getNextDueMatureCards(i);
    }
    if (i > 0 && cumulative) {
      series[i]=count + series[i - 1];
    }
 else {
      series[i]=count;
    }
  }
  return series;
}
